{
    "name": "magroski/bob-ross",
    "description": "An easy way to upload, manipulate and persist images.",
    "type": "library",
    "keywords": [
        "Image",
        "Upload"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Lucas Magroski",
            "email": "magroski@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "BobRoss\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WT\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": "^7.1 || ^8",
        "aws/aws-sdk-php": "^3.27"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "phpstan/phpstan": "^0.10.0",
        "object-calisthenics/phpcs-calisthenics-rules": "^3.1",
        "roave/security-advisories": "dev-master",
        "slevomat/coding-standard": "4.5",
        "squizlabs/php_codesniffer": "*"
    }
}
